using System;
class Program
{
    static void Main()
    {
        Random rd = new Random();
        int rand_num = rd.Next(1, 100);
        Console.WriteLine(rand_num);
        int number = 0;
        while(number<10)
        {
            Console.WriteLine("Enter your guess number");
            int guess_number = int.Parse(Console.ReadLine());
            if(guess_number == rand_num)
            {
                Console.WriteLine("You Won");
                break;
            }
            if(number==9)
            {
                Console.WriteLine("You Lose");
            }
            else
            {
                Console.WriteLine("");
            }
            number++;
            
        }

        
    }
}
