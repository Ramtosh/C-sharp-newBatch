using System;

namespace ConsoleApp38
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Enter any number:");
           int n = int.Parse(Console.ReadLine());
            if (n % 2 == 0)
            {
                Console.WriteLine("Number is Even:");
            }
            else
            {
                Console.WriteLine("Number is odd:");
            }
        }
    }
}
