 class Demo 
    {
        int num1;
        int num2;
        int result;
        void add() {
            result = num1 + num2;
            Console.WriteLine(result);
            Console.ReadLine();
        }
        void subtruct()
        {
            result = num1 - num2;
            Console.WriteLine(result);
            Console.ReadLine();
        }
        static void Main(string[] args)
        {
            Demo obj = new Demo();
            obj.num1 = 10;
            obj.num2 = 20;
            obj.add();
            obj.subtruct();
        }
    }
